<p id="notice"><%= notice %></p>

<h1>Reservas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Nif</th>
      <th>Direccion</th>
      <th>Coche</th>
      <th>Kilometraje</th>
      <th>Matriculacion</th>
      <th>Chasis</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reservas.each do |reserva| %>
      <tr>
        <td><%= reserva.nombre %></td>
        <td><%= reserva.nif %></td>
        <td><%= reserva.direccion %></td>
        <td><%= reserva.coche %></td>
        <td><%= reserva.kilometraje %></td>
        <td><%= reserva.matriculacion %></td>
        <td><%= reserva.chasis %></td>
        <td><%= reserva.precio %></td>
        <td><%= link_to 'Show', reserva %></td>
        <td><%= link_to 'Edit', edit_reserva_path(reserva) %></td>
        <td><%= link_to 'Destroy', reserva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reserva', new_reserva_path %>
